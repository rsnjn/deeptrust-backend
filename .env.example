# DeepTrust Backend Environment Variables

# Server Configuration
PORT=3000
NODE_ENV=production

# OpenAI API (for LLM explanations) - Optional
OPENAI_API_KEY=your_openai_api_key_here

# PyDeepFakeDet Model Configuration
MODEL_PATH=./models/pydeepfakedet_model.pth
CONFIDENCE_THRESHOLD=30

# Rate Limiting
RATE_LIMIT_REQUESTS=10
RATE_LIMIT_WINDOW=60

# CORS Configuration
ALLOWED_ORIGINS=chrome-extension://,https://

# Logging
LOG_LEVEL=info
